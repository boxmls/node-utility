<table class="main" width="100%" cellpadding="0" cellspacing="0" style="font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif; box-sizing: border-box; font-size: 14px; margin: 0; padding: 0 0 20px 0;">
    <tr>
        <td width="12" bgcolor="#f28d19"><img src="<%= S3bucketLink %>/email/blank.gif" width="12" height="1" /></td>
        <td width="90"><img
                src="<% if( agent.profilePhoto ) { %><%= agent.profilePhoto %><% } else { %><%= S3bucketLink %>/email/default-avatar.png<% } %>"
                width="90" height="90" /></td>
        <td width="36"><img src="<%= S3bucketLink %>/email/blank.gif" width="36" height="1" /></td>
        <td width="200"><span style="font-size: 20px; color: #3b3b3b; line-height: 30px"><%= agent.firstName %> <%= agent.lastName %></span><br />
            <span style="font-size: 12px; color: #686868"><%= agent.agent.office.name %><br />
            License #<%= agent.agent.mls.licNum %></span></td>
        <td width="45"><img src="<%= S3bucketLink %>/email/blank.gif" width="45" height="1" /></td>
        <td><span style="font-size: 20px; line-height: 30px">&nbsp;</span><br />
            <span style="font-size: 12px; color: #686868"><a href="mailto:<%= agent.email %>" style="color: #0069ba; text-decoration: none"><%= agent.email %></a><br />
            <% if( agent.agent.contact.phones[0] ) { %><%= agent.agent.contact.phones[0].phoneComplete %><% } %></span></td>
    </tr>
</table>